<h1>Listing shipping_rates</h1>

<table>
  <tr>
    <th>From zip code</th>
    <th>To zip code</th>
    <th>Weight</th>
    <th>Ship date</th>
    <th>Service type</th>
    <th>Package type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shipping_rates.each do |shipping_rate| %>
  <tr>
    <td><%= shipping_rate.from_zip_code %></td>
    <td><%= shipping_rate.to_zip_code %></td>
    <td><%= shipping_rate.weight %></td>
    <td><%= shipping_rate.ship_date %></td>
    <td><%= shipping_rate.service_type %></td>
    <td><%= shipping_rate.package_type %></td>
    <td><%= link_to 'Show', shipping_rate %></td>
    <td><%= link_to 'Edit', edit_shipping_rate_path(shipping_rate) %></td>
    <td><%= link_to 'Destroy', shipping_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Shipping rate', new_shipping_rate_path %>
